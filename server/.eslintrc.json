{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-undef": "error",
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}
